    </bag>-->
    <!--<bag name="Fin_Recebers" fetch="join">
      <key column="Cod_Estabelecimento" />
    </bag> -->
    <!--<bag name="Valores_Fixoss" fetch="join">
      <key column="Cod_Estabelecimento" />
      <one-to-many class="Valores_Fixos" />
    </bag>-->
  </class>
  
</hibernate-mapping>